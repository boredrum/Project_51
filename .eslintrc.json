{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {"jsx": true}
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [
        "webpack.config.js"
    ],
    "rules": {
        "no-unused-vars":"error",
        "no-console":"warn",
        "quotes":["warn", "single"],
        "jsx-quotes":["error", "prefer-double"],
        "prefer-const":"error",
        "indent":["warn", 2],
        "comma-dangle":["warn", "always"],
        "semi":["warn", "always"]
    }
}
